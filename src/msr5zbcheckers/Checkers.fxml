<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="gameVBox" prefHeight="450.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="msr5zbcheckers.CheckersController">
  <children>
    <MenuBar fx:id="menu" VBox.vgrow="ALWAYS">
      <menus>
        <Menu mnemonicParsing="false" text="Grid">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#setGrid16x16" text="16x16" />
                  <MenuItem mnemonicParsing="false" onAction="#setGrid10x10" text="10x10" />
                  <MenuItem mnemonicParsing="false" onAction="#setGrid8x8" text="8x8" />
                  <MenuItem mnemonicParsing="false" onAction="#setGrid3x3" text="3x3" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Colors">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#setColorDefault" text="Default" />
                  <MenuItem mnemonicParsing="false" onAction="#setColorBlue" text="Blue" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
      <AnchorPane fx:id="board" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
  </children>
</VBox>
